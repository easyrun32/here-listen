{
  "containerDefinitions": [
    {
      "name": "client",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/here-listen-client:prod",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "environment": [
        {
          "name": "REACT_APP_SECRETME",
          "value": "%s"
        }
      ]
    }
  ],
  "family": "here-listen-client-td"
}
